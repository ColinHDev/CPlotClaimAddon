{
	"name": "colinhdev/cplotclaimaddon",
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"pocketmine/pocketmine-mp": "^5.0.0",
		"colinhdev/cplot": "dev-development",
		"sof3/await-generator": "^3.4.1"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.6.7",
		"phpstan/phpstan-strict-rules": "^1.2.3",
		"phpstan/extension-installer": "^1.1"
	},
	"repositories": {
		"cplot": {
			"type": "vcs",
			"url": "https://github.com/ColinHDev/CPlot"
		},
		"bedrockeconomy": {
			"type": "package",
			"package": {
				"name": "cooldogedev/bedrockeconomy",
				"version": "2.0.8",
				"source": {
					"type": "git",
					"url": "https://github.com/cooldogedev/BedrockEconomy",
					"reference": "main"
				}
			}
		}
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}